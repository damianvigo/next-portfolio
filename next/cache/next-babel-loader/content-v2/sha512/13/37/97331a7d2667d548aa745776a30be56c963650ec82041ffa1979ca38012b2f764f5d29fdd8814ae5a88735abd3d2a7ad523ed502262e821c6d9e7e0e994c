{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport Navbar from './Navbar';\nimport { useEffect } from 'react';\nimport { useRouter } from 'next/router';\nimport NProgress from 'nprogress';\nimport ClassNames from 'classnames';\nimport Title from './Title';\n\nconst Layout = ({\n  title,\n  children,\n  titleHead,\n  footer = true,\n  dark = false\n}) => {\n  const router = useRouter();\n  useEffect(() => {\n    const handleRouteChange = url => {\n      // console.log(url);\n      NProgress.start();\n    };\n\n    router.events.on('routeChangeStart', handleRouteChange);\n    router.events.on('routeChangeComplete', () => NProgress.done());\n    return () => {\n      router.events.off('routeChangeStart', handleRouteChange);\n    };\n  }, []);\n  return (\n    /*#__PURE__*/\n    // <div className={dark ? 'bg-dark' : ''}>\n    _jsxs(\"div\", {\n      className: ClassNames({\n        'bg-dark': dark,\n        'bg-light': !dark\n      }),\n      children: [/*#__PURE__*/_jsx(Title, {\n        titleHead: titleHead\n      }), /*#__PURE__*/_jsx(Navbar, {}), /*#__PURE__*/_jsxs(\"main\", {\n        className: \"container py-4\",\n        children: [title && /*#__PURE__*/_jsx(\"h1\", {\n          className: ClassNames('text-center mb-5', {\n            'text-light': dark\n          }),\n          children: title\n        }), children]\n      }), footer && /*#__PURE__*/_jsx(\"footer\", {\n        className: \"bg-dark text-light text-center\",\n        children: /*#__PURE__*/_jsxs(\"div\", {\n          className: \"container p-4\",\n          children: [/*#__PURE__*/_jsx(\"h2\", {\n            children: \"\\xA9 Ryan Ray Portfolio\"\n          }), /*#__PURE__*/_jsxs(\"p\", {\n            children: [\"2000 - \", new Date().getFullYear()]\n          }), /*#__PURE__*/_jsx(\"small\", {\n            children: \"All rights Reserverd.\"\n          })]\n        })\n      })]\n    })\n  );\n};\n\nexport default Layout;","map":null,"metadata":{},"sourceType":"module"}